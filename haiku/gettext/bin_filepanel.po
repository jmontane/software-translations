#: FilePanelApp
msgid "display a load/save file paneln"
msgstr "mostra un quadre per carregar o desar un fitxern"

#: FilePanelApp
msgid "shorttlongtdescriptionn"
msgstr "descripciót curtatllargan"

#: FilePanelApp
msgid "%s not a valid optionn"
msgstr "%s no és una opció vàlidan"

#: FilePanelApp
msgid "-st--savetuse a save FilePaneln"
msgstr "-st--savetfusa un quadre de desament del FilePaneln"

#: FilePanelApp
msgid "%s: this option requires a parametern"
msgstr "%s: amb aquesta opció cal un paràmetren"

#: FilePanelApp
msgid "-ht--helptdisplay usagen"
msgstr "-ht--helptmostra'n l'úsn"

#: FilePanelApp
msgid "-lt--loadtuse a load FilePanel (default)n"
msgstr "-lt--loadtusa un quadre de càrrega del FilePanel (per defecte)n"

#: FilePanelApp
msgid "-nt--nametset the default name for savingn"
msgstr "-nt--nametestableix el nom per defecte per desar-hon"

#: FilePanelApp
msgid ""
"-kt--kindtkind of entries that can be opened (flavour): any combination of "
"f, d, s (file (default), directory, symlink)n"
msgstr ""
"-kt--kindttipus d'entrades que es poden obrir (essència): qualsevol "
"combinació de f, d, s (fitxer (per defecte), carpeta, enllaç simbòlic)n"

#: FilePanelApp
msgid "%s: UNIMPLEMENTEDn"
msgstr "%s: NO ESTÀ SUPORTATn"

#: FilePanelApp
msgid "-tt--titletset the FilePanel window titlen"
msgstr "-tt--titletiestableix el títol de finestra del FilePaneln"

#: FilePanelApp
msgid "-ft--forbidtunimplementedn"
msgstr "-ft--forbidtno està suportatn"

#: FilePanelApp
msgid ""
"usage: %s [-h]     [-d folder]              [-l|-s]     [-t ttl]      [-1]"
"       [-m]      [-a pattern]      [-f pattern]n"
msgstr ""
"ús: %s [-h]     [-d carpeta]              [-l|-s]     [-t títol]      [-1]"
"       [-m]      [-a plantilla_acceptable]      [-f plantilla_inacceptable]n"

#: FilePanelApp
msgid "-1t--singletallow only 1 file to be selectedn"
msgstr "-1t--singletpermet seleccionar 1 sol fitxern"

#: FilePanelApp
msgid "-at--allowtunimplementedn"
msgstr "-at--allowtno està suportatn"

#: FilePanelApp
msgid "-mt--modaltmakes the FilePanel modaln"
msgstr "-mt--modaltfa que el FilePanel sigui modaln"

#: FilePanelApp
msgid "options:n"
msgstr "opcions:n"

#: FilePanelApp
msgid "-dt--directorytopen at <folder>n"
msgstr "-dt--directorytobre a <carpeta>n"

#: FilePanelApp
msgid ""
"usage: %s [--help] [--directory folder] [--load|--save] [--title ttl] "
"[--single] [--modal] [--allow pattern] [--forbid pattern]n"
msgstr ""
"ús: %s [--help] [--directory carpeta] [--load|--save] [--title títol] "
"[--single] [--modal] [--allow plantilla_acceptable] [--forbid "
"plantilla_inacceptable]n"
